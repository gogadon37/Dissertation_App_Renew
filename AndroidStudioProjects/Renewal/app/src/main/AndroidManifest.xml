<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gogadon.renewal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddItemActivity"></activity>
        <activity android:name=".Dashboard_Fragment" />
        <activity android:name=".Dashboard" />
        <activity android:name=".termsandconditions_fragment" />
        <activity android:name=".NotificationTimeDialog" />
        <activity android:name=".RemindersFragment" />
        <activity android:name="com.gogadon.fragments.UserDetailsFragment" />
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>